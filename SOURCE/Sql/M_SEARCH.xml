<?xml version="1.0" encoding="utf-8" ?>
<System>
	<!-- Form Search MEmp -->
	<M_EMP001>
	SELECT M.EMP_CODE
		 , M.EMP_NAME
		 , M.EMP_KANA
		 , M.EMP_TNAME
		 , M.SEARCHIDX
		 , M.EMP_USER
		 , M.USE_FLG
		 , M.SECT_CODE
		 , M.SECT1_CODE
		 , M.SECT2_CODE
		 , M.SECT3_CODE
		 , M.SECT4_CODE
	  FROM M_EMP AS M
	</M_EMP001>

   	<!-- Form Search MCust -->
	<M_CUST001>
    SELECT M.CUST_CODE
		 , RTRIM(COALESCE(M.CUSTR_NAME,'')) AS CUST_RNAME
		 , (RTRIM(COALESCE(M.CUST1_NAME,'')) + ' ' + RTRIM(COALESCE(M.CUST2_NAME,''))) AS CUST_NAME
		 , M.USE_FLG
		 , RTRIM(COALESCE(M.CUST_KANA,'')) AS CUST_KANA
		 , RTRIM(COALESCE(M.CUSTT_NAME,'')) AS CUST_TNAME
	  FROM M_CUST AS M
	</M_CUST001>

	<!-- Form Search MPost -->
	<M_POST001>
	SELECT M.F1
		 , M.F2,
